{
    "name": "zortje/boxcar-notifications-php",
    "description": "A Boxcar api wrapper for sending notifications to a Boxcar account",
    "type": "library",
    "keywords": [
        "boxcar",
        "notifications"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "René Tønner Stokholm",
            "email": "zortje@gmail.com",
            "homepage": "http://www.renestokholm.dk",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zortje/boxcar-notifications-php/issues"
    },
    "require": {
        "php": ">=5.5",
        "lib-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Zortje\\BoxcarNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zortje\\BoxcarNotifications\\Tests\\": "tests/"
        }
    }
}
